{
  "name": "WHEEL",
  "version": "2.0.1",
  "description": "Scientific Workflow system",
  "main": "app/index.js",
  "bin": {
    "wheel": "app/cmd.js"
  },
  "author": {
    "name": "Kenji Ono"
  },
  "contributors": [
    "Tomohiro Kawanabe",
    "Kazuya Tsuruta",
    "Naoyuki Sogo",
    "Yuki Tanaka"
  ],
  "license": "BSD-2-Clause",
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "start": "node app/index",
    "buildImage": "cross-env DOCKER_BUILDKIT=1 docker build --target=exec -t wheel .",
    "mocha": "mocha",
    "lint": "eslint --fix app/**/*.js test/UT/**/*.js",
    "UT:server": "cross-env WHEEL_DISABLE_LOG=1  WHEEL_DISABLE_CONSOLE_LOG=1 mocha --recursive test/UT/server/ --unhandled-rejections=strict",
    "UT:client": "opener test/UT/client/index.html",
    "coverage:server": "nyc --reporter=html --reporter=text npm run UT:server",
    "coverage:client": "nyc --reporter=html --reporter=text npm run UT:client",
    "E2E": "wdio run test/E2E/wdio.conf.js",
    "prepare": "webpack",
    "watch": "webpack -w",
    "node-dev": "node-dev --inspect app/index",
    "dev": "run-p watch node-dev"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run UT:server"
    }
  },
  "lint-staged": {
    "*.js": "eslint --fix"
  },
  "files": [
    "app",
    "package.json"
  ],
  "dependencies": {
    "abc4": "^0.11.5",
    "ajv": "^6.12.6",
    "arssh2-client": "^2.3.0",
    "body-parser": "^1.19.0",
    "cookie": "^0.4.1",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "cross-env": "^6.0.3",
    "debug": "^4.2.0",
    "express": "^4.17.1",
    "file-type": "^13.1.0",
    "fs-extra": "=5.0.0",
    "glob": "^7.1.6",
    "is-path-inside": "^3.0.2",
    "klaw": "^3.0.0",
    "log4js": "^6.4.2",
    "minimatch": "^3.0.5",
    "nunjucks": "^3.2.1",
    "promise-retry": "^1.1.1",
    "read-chunk": "^3.2.0",
    "simple-batch-system": "^1.8.2",
    "socket.io": "^4.4.1",
    "socketio-file-upload": "^0.7.0",
    "ssh2": "^1.6.0",
    "stat-mode": "^1.0.0",
    "uuid": "^3.4.0"
  },
  "devDependencies": {
    "@wdio/cli": "^7.16.16",
    "@wdio/local-runner": "^5.23.0",
    "@wdio/mocha-framework": "^7.16.15",
    "@wdio/selenium-standalone-service": "^5.16.10",
    "@wdio/spec-reporter": "^5.18.6",
    "@wdio/sync": "^5.18.6",
    "ace-builds": "^1.4.8",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "chai-fs": "^2.0.0",
    "chai-iterator": "^3.0.2",
    "chai-json-schema": "^1.5.1",
    "chai-webdriverio": "^1.0.0",
    "chokidar-cli": "^2.1.0",
    "chromedriver": "^98.0.1",
    "css-loader": "^3.4.2",
    "deep-equal-in-any-order": "^1.0.28",
    "eslint": "^8.10.0",
    "eslint-config-eslint": "^6.0.0",
    "eslint-plugin-chai-expect": "^2.1.0",
    "eslint-plugin-chai-friendly": "^0.5.0",
    "eslint-plugin-jsdoc": "^20.3.1",
    "eslint-plugin-node": "^11.0.0",
    "eslint-watch": "^6.0.1",
    "file-loader": "^5.0.2",
    "font-awesome": "^4.7.0",
    "husky": "^7.0.4",
    "jquery": "^3.5.1",
    "jquery-contextmenu": "^2.9.0",
    "jquery-ui": "^1.13.1",
    "js-cookie": "^2.2.1",
    "jsdoc-to-markdown": "^7.1.1",
    "json-loader": "^0.5.7",
    "lint-staged": "^10.4.0",
    "markdown-pdf": "^10.0.0",
    "marked": "^4.0.12",
    "material-design-icons-iconfont": "^5.0.1",
    "mini-css-extract-plugin": "^0.9.0",
    "mocha": "^9.2.1",
    "nlf": "^2.1.1",
    "node-dev": "^7.1.0",
    "npm-check-updates": "^4.0.1",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "rewire": "^6.0.0",
    "selenium-server": "^3.141.59",
    "sinon": "^8.1.0",
    "sinon-chai": "^3.4.0",
    "split.js": "^1.5.11",
    "style-loader": "^1.1.2",
    "svg.draggable.js": "^2.2.1",
    "svgjs": "^2.6.2",
    "terser-webpack-plugin": "^2.3.4",
    "url-loader": "^3.0.0",
    "viewerjs": "^1.5.0",
    "vue": "^2.6.11",
    "vuetify": "^2.2.4",
    "wdio-chromedriver-service": "^5.0.2",
    "wdio-json-reporter": "^1.3.1",
    "webdriverio": "^5.18.5",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10"
  }
}
